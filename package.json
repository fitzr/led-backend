{
  "name": "led-backend",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "bin": {
    "led-backend": "src/bin/led-backend.js"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint src test --ext .ts --ignore-pattern '**/*.d.ts'",
    "lint:fix": "eslint src test --ext .ts --ignore-pattern '**/*.d.ts' --fix",
    "synth:dev": "tsc && cdk synth -c env=dev",
    "diff:dev": "tsc && cdk diff -c env=dev",
    "deploy:dev": "tsc && cdk deploy -c env=dev"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.25.0",
    "@types/aws-lambda": "^8.10.40",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "aws-cdk": "1.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.25.0",
    "@aws-cdk/aws-iam": "^1.25.0",
    "@aws-cdk/aws-iot": "^1.25.0",
    "@aws-cdk/aws-lambda": "^1.25.0",
    "@aws-cdk/core": "1.25.0",
    "aws-sdk": "^2.619.0",
    "source-map-support": "^0.5.16"
  }
}
